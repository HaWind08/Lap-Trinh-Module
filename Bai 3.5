#include<iostream>
#include<math.h>
using namespace std;

int giaiHePt(int a, int b, int c, int d, int e, int f, int& x, int& y)
{
	int det = a * e - b * d;
	int det_x = c * e - f * b;
	int det_y = a * f - d * c;
	if (det == 0)
	{
		if (det_x == 0 && det_y == 0)
		{
			return -1;
		}
		else
		{
			return 0;
		}
	}
	else
	{
		x = det_x / det;
		y = det_y / det;
		return 1;
	}
}
int main()
{
	int a, b, c, d, e, f, x, y;
	cout << "Nhap cac he so cua phuong trinh: " << endl;
	cin >> a >> b >> c >> d >> e >> f;
	int K = giaiHePt(a, b, c, d, e, f, x, y);
	if (K == 0)
	{
		cout << "He phuong trinh vo nghiem" << endl;
	}
	else if (K == -1)
	{
		cout << "He phuong trinh vo so nghiem" << endl;
	}
	else
	{
		cout << "He phuong trinh co nghiem duy nhat" << endl;
		cout << "x = " << x << endl;
		cout << "y = " << y << endl;
	}
	return 0;
}
